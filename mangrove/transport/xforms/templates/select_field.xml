<{{ field.type }} ref="{{ field.code }}" appearance="quick">
    <label>{{ field.label }}</label>
    <hint>Choose answer(s) from the list.</hint>
    {% for option in field.options %}
        <item>
            <label>{{ "".join(option.get('text')) }}</label>
            <value>{{ option.get('val') }}</value>
        </item>
    {% endfor %}

</{{ field.type }}>
